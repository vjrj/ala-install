{
  "LA_project_name": "Living Atlas",
  "LA_repo_name": "{{ cookiecutter.LA_project_name|replace(' ', '-') }}",
  "LA_pkg_name": "{{ cookiecutter.LA_repo_name|lower |replace(' ', '-')  }}",
  "LA_domain": "{{ cookiecutter.LA_pkg_name }}.org",
  "LA_collectory_uses_subdomain": "yes",
  "LA_collectory_subdomain": "{{ '%s.%s'|format('collectory', cookiecutter.LA_domain) if cookiecutter.LA_collectory_uses_subdomain == 'yes' else '{{ cookiecutter.LA_domain }}' }}",
  "LA_collectory_context": "{{ '' if cookiecutter.LA_collectory_uses_subdomain == 'yes' else '/collectory' }}",
  "enable_ssl": "no",
  "use_CAS": "no",
  "use_regions": "no",
  "use_spatial": "no",
  "use_especies": "no",
  "use_species_lists": "no"
}
